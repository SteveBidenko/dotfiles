#!/bin/bash

alias ls='ls --color'
alias ll='ls -l'
alias lz='ll -Shr'
alias lzd='ll -Sh'
# alias p='ps -eFl | grep -i'
alias p='ps -eFl | grep -i'
alias f='find ./ -name'
alias aur='yaourt --noconfirm'
alias lm='lsmod | grep -i'
alias di='du -csh'
alias n='nautilus .'
alias ww='watch --interval=0'
alias grep='grep --color="always"'
alias rm='rm -i'
alias mt='perl -nle '"'"'BEGIN {$|++} print scalar(localtime), " | ", $_'"'"''
alias fuck='$(thefuck $(fc -ln -1))'
alias g='grep -i'
alias vg='vagrant'
alias o='xdg-open'
alias diff='diff --color=always'
# alias p='ps aux | grep -i'
alias sc='sudo systemctl'
alias jc='sudo journalctl'
alias k='killall -9'
alias up='curl -X POST http://kv.dun.ai/ips/latitude --data value=`curl canihazip.com/s`'
